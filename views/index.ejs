<% include ./partials/header %>
<h1>INDEX PAGE!!</h1>


    <% blogs.forEach(function(blog){%>
        <div>
            <h2><%= blog.title %></h2>
            <img src="<%=blog.image %>" >
            <span><%= blog.created%></span>
            <p><%=blog.body.substring(0,100) %>...</p>
            <div>
                <p>
                    <a href="/blogs/<%= blog._id %>" class= "ui blue basic button">Read More</a>
                </p>
            </div>
        </div>
    <% }) %>

<% include ./partials/footer %>